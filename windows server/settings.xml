    <type>msi</type>
  </applications>

  <applications name="mysql">
    <name>mysql</name>
    <version>MySQL Server 5.5</version>
    <url>http://cdn.mysql.com/Downloads/MySQL-5.5/mysql-5.5.28-winx64.msi</url>
    <binary>mysql-5.5.28-winx64.msi</binary>
    <type>msi</type>
  </applications>

  <applications name="mongodb">
    <name>mongodb</name>
    <url>http://downloads.mongodb.org/win32/mongodb-win32-x86_64-2008plus-2.2.0.zip</url>
    <binary>mongodb-win32-x86_64-2008plus-2.2.0.zip</binary>
    <type>zip</type>
    <directories>
      <directory>C:\var\log\mongodb</directory>
      <directory>C:\var\lib\mongodb</directory>
    </directories>
  </applications>

  <applications name="elasticsearch">
    <name>elasticsearch</name>
    <url>http://cloud.github.com/downloads/elasticsearch/elasticsearch/elasticsearch-0.19.9.zip</url>
    <binary>elasticsearch-0.19.9.zip</binary>
    <type>zip</type>
  </applications>

  <applications name="elasticsearch-service-wrapper">
    <name>Elasticsearch Service Wrapper</name>
