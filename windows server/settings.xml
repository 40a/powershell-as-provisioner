  </applications>

  <applications name="apache">
    <filename>Apache2.2</filename>
    <url>http://mirror.netcologne.de/apache.org/httpd/binaries/win32/httpd-2.2.22-win32-x86-openssl-0.9.8t.msi</url>
    <binary>httpd-2.2.22-win32-x86-openssl-0.9.8t.msi</binary>
