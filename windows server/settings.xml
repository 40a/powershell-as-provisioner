<?xml version="1.0" encoding="utf-8" ?>

<settings>
  <common>
    <directories>
      <download>C:\Users\Administrator\Downloads</download>
      <program>C:\Program Files</program>
    </directories>
  </common>

  <applications name="java">
    <name>Java</name>
    <url>http://javadl.sun.com/webapps/download/AutoDL?BundleId=68736</url>
    <binary>jre-7u7-windows-x64.exe</binary>
    <type>exec</type>
  </applications>

  <applications name="7zip">
    <name>7zip</name>
    <!-- origin: http://downloads.sourceforge.net/project/sevenzip/7-Zip/9.20/7z920-x64.msi?r=http%3A%2F%2Fwww.7-zip.org%2F&ts=1349696827&use_mirror=freefr-->
    <url>http://downloads.sourceforge.net/project/sevenzip/7-Zip/9.20/7z920-x64.msi?r=http%3A%2F%2Fwww.7-zip.org%2F&amp;ts=1349696827&amp;use_mirror=freefr</url>
